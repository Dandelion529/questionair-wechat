/* pages/login/login.wxss */
.loading {
    position: relative;
    z-index: 99;
    width: 100%;
    height: 100vh;
    background: #FFF;
}

.loading-gongfu {
    position: absolute;
    z-index: 99;
    left: 50%;
    top: 50%;
    width: 100%;
    height: 100%;
    transform: translate(-50%,-50%);
}

.loginBtn {
    position: absolute;
    bottom: 175rpx;
    left: 50rpx;
    z-index: 100;
    width: 440rpx;
    height: 85rpx;
    margin-left: 17%;
    background-color: #fff;
    border-radius: 50rpx;
}

.goBtn {
    position: absolute;
    top: 0;
    left: -20rpx;
    width: 483rpx;
    height: 129rpx;
    
}